{
    "source": {
      "include": ["src"],
      "exclude": ["node_modules"],
      "includePattern": ".js$|.ts$|.tsx$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs",
      "recurse": true,
      "template": "templates/default"
    }
  }
  